{
    //#region | Colour display settings | <-- Themes, colours, fonts, semantic highlighting, etc.

    "workbench.colorTheme": "Dracula Theme",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Python - Docstring text",
                "scope": [
                    "comment.block.cs",
                    "string.quoted.docstring.multi",
                    "string.quoted.docstring.multi.python punctuation.definition.string.begin",
                    "string.quoted.docstring.multi.python punctuation.definition.string.end",
                    "string.quoted.docstring.multi.python constant.character.escape"
                ],
                "settings": {
                    "foreground": "#71e089"
                }
            },
            {
                "name": "Namespaces",
                "scope": [
                    "entity.name.namespace",
                    "entity.name.type.namespace.cs",
                ],
                "settings": {
                    "foreground": "#56cfff",
                    "fontStyle": "italic bold"
                }
            },
            {
                "name": "Python - Comment text",
                "scope": [
                    "punctuation.definition.comment.cs",
                    "comment.line.double-slash.cs",
                    "comment.line.number-sign.python",
                    "comment.line.number-sign.python punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#83c6c9"
                }
            },
            {
                "name": "Python - Property",
                "scope": [
                    "variable.other.property"
                ],
                "settings": {
                    "foreground": "#F1FA8C",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "variable.other.object.cs"
                ],
                "settings": {
                    "foreground": "#8BE9FD"
                }
            },
            {
                "name": "C# - Function",
                "scope": [
                    "entity.name.function.cs",
                    "entity.name.type.class.cs",
                ],
                "settings": {
                    "foreground": "#50FA7B"
                }
            }

        ]
    },
    "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono, monospace",

    "[python]": {
        "editor.semanticHighlighting.enabled": true,
    },
    "[csharp]": {
        "editor.semanticHighlighting.enabled": false,
    },
